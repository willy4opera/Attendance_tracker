# YouTube API Configuration
# Copy this to your .env file and fill in the values

# OAuth 2.0 Credentials (from Google Cloud Console)
YOUTUBE_CLIENT_ID=your_client_id_here.apps.googleusercontent.com
YOUTUBE_CLIENT_SECRET=your_client_secret_here
YOUTUBE_REDIRECT_URI=http://localhost:3000/api/auth/youtube/callback

# Optional: API Key for public data access
YOUTUBE_API_KEY=your_api_key_here

# File Upload Settings
MAX_VIDEO_SIZE=134217728  # 128MB in bytes
TEMP_UPLOAD_DIR=/tmp/video-uploads
